    *)
    	s=" "
        if test x"$ac_new_flags" = x""; then
            s="";
	fi
        if test x"$l" = x""; then
            ac_new_flags="$ac_new_flags$s$i";
        else
            ac_new_flags="$ac_new_flags$s$l $i";
        fi
        l=""
        ;;
    esac
  done
  EXECINFO_LDFLAGS=$ac_new_flags


  ac_new_flags=""
  for i in $EXECINFO_CFLAGS; do
    case $i in
    -I/usr/include|-I/usr/include/) ;;
    *) ac_new_flags="$ac_new_flags $i" ;;
    esac
  done
