	      -o $@ @SONAMEFLAG@`basename $@`
# Linker command to link samba internal plugin modules:
SHLD_MODULE = $(SHLD) $(LDSHFLAGS_MODULES) $(MODULE_EXPORTS) \
	-o $@ @SONAMEFLAG@`basename $@`

