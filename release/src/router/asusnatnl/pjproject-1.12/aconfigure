# Is the header present?
{ echo "$as_me:$LINENO: checking sys/soundcard.h presence" >&5
echo $ECHO_N "checking sys/soundcard.h presence... $ECHO_C" >&6; }
cat >conftest.$ac_ext <<_ACEOF
