  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  ac_cv_header_linux_soundcard_h=$ac_header_preproc
fi
{ echo "$as_me:$LINENO: result: $ac_cv_header_linux_soundcard_h" >&5
echo "${ECHO_T}$ac_cv_header_linux_soundcard_h" >&6; }

