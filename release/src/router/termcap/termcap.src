	:am:bs:\
	:co#80:li#24:\
	:al=2.3*\001<2.3*/>:bl=^G:cd=2.2*\037:ce=.1*\036:\
	:cl=12\014:cm=\024%+^X%>/0%+P:cr=^M:dc=.1*\004:\
