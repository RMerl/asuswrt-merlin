#. * inserted verbatim.
#: src/text.c:3534
msgid "Verbatim Input"
msgstr "Буквальний ввід"

#: src/text.c:3730
msgid "No further matches"
msgstr "Далі немає відповідників"

#. TRANSLATORS: Shown when there are zero possible completions.
#: src/text.c:3734
msgid "No matches"
msgstr "Немає відповідників"

