	struct tm *tm;
	if (sizeof(time_t) == 8) {
		time_t max_time = 0x7fffffffffffffffll;
		tm = gmtime(&max_time);
		/* This should fail with 32-bit tm_year. */
		if (tm == NULL) {
			/* Max time_t that works with 32-bit int tm_year in struct tm. */
			max_time = 67768036191676799ll;
			tm = gmtime(&max_time);
			if (tm) {
				exit(0);
			}
		}
	}
	exit(1);
}
