;;			A6	64 ::ffff:ffff:ffff:ffff foo.
;;			A6	127 ::1 foo.
;;			A6	128  .
aaaa01			AAAA	ffff:ffff:ffff:ffff:ffff:ffff:ffff:ffff
aaaa02			AAAA	::1
afsdb01			AFSDB	0 hostname
afsdb02			AFSDB	65535 .
$TTL 300	; 5 minutes
