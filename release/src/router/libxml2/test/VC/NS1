<!DOCTYPE ns:doc [
<!ELEMENT ns:doc (#PCDATA | ns:a | ns:b)*>
<!ATTLIST ns:doc
          ns:attr  CDATA #REQUIRED
          xmlns:ns CDATA #IMPLIED>
<!ELEMENT ns:a EMPTY>
<!ELEMENT ns:b (#PCDATA)>
]>
<ns:doc ns:attr="val" xmlns:ns="http://www.example.org/test/">
   <ns:a/>
   <ns:b>first line</ns:b>
   <ns:b>second line</ns:b>
</ns:doc>
