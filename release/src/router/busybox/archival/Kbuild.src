lib-$(CONFIG_BZIP2)		+= bzip2.o bbunzip.o

lib-$(CONFIG_UNXZ)		+= bbunzip.o
lib-$(CONFIG_UNLZMA)		+= bbunzip.o
lib-$(CONFIG_BUNZIP2)		+= bbunzip.o
lib-$(CONFIG_GUNZIP)		+= bbunzip.o
lib-$(CONFIG_UNCOMPRESS)	+= bbunzip.o
